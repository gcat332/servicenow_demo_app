        <name>Check Duplicate Reservation</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }
	var ga = new GlideAjax('checkDupCar');
	ga.addParam('sysparm_name', 'chkDupCar');
	ga.addParam('sysparm_vehicle_number', g_form.getValue("vehicle_number"));
    ga.addParam('sysparm_start_date', g_form.getValue("start_date"));
	ga.addParam('sysparm_end_date', g_form.getValue("end_date"));
    ga.getXML(chkDup);


    function chkDup(response) {
	var answer = response.responseXML.documentElement.getAttribute("answer");
	if (answer == 'false')
		{ alert("Your car already reserved.");
          g_form.clearValue('vehicle_number');}
	}
}]]></script>
