        <name>calculateDuration</name>
        <script><![CDATA[var calculateDuration = Class.create();
calculateDuration.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
	calDuration: function(){
		var start = new GlideDateTime(this.getParameter('sysparm_start_date'));
		var end = new GlideDateTime(this.getParameter('sysparm_end_date'));
        var dur = GlideDateTime.subtract(start, end).getDisplayValue();
		return dur;
	},
	isPublic: true,
    type: 'calculateDuration'
});]]></script>
